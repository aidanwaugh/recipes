{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "func-names": "warn",
    "object-curly-newline": "error",
    "max-len": "off",
    "no-return-assign": 0
  }
}
